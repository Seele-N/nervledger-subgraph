    "create-local": "graph create --node http://localhost:8020/ /snp/snp",
    "remove-local": "graph remove --node http://localhost:8020/ /snp/snp",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 /snp/snp"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.0",
    "typescript": "^3.5.2"
  }
