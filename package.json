  "version": "0.0.2",
  "author": "Snp",
  "scripts": {
    "codegen": "graph codegen",
    "codegen:snp": "graph codegen subgraphs/snp.yaml",
    "build": "graph build",
    "build:snp": "graph build subgraphs/snp.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
